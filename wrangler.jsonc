  "name": "athletetime",
  "compatibility_date": "2025-09-01",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "athletetime-production",
      "database_id": "temp-local-db-id"
    }
  ]
}